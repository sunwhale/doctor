  year={2009},
  keywords={A. Aluminides, miscellaneous,B. Mechanical properties at high temperatures,D. Defects: dislocation geometry and arrangement,E. Mechanical properties, theory,F. Mechanical testing},
  title={Thermomechanical fatigue behaviour of a third generation ??-TiAl intermetallic alloy},
  journal={Intermetallics},
}
@article{Becker2011596,
  doi={http://dx.doi.org/10.1016/j.ijplas.2010.08.005},
  author={M Becker and H-P Hackenberg},
  title={A constitutive model for rate dependent and rate independent inelasticity. Application to {IN718}},
  url={http://www.sciencedirect.com/science/article/pii/S0749641910001105},
  journal={International Journal of Plasticity},
  issn={0749-6419},
  number={4},
  abstract={In this paper, a constitutive model for the unified description of rate dependent and rate independent material behavior is proposed. It is applicable to isotropic metals subjected to arbitrary thermomechanical loading conditions at small strains. The focus of the model formulation is its validity for the full range of thermal and mechanical loading conditions to be covered in an industrial context by e.g. modern aero-engine designs. Consequently, the proposed model describes the material behavior on a macroscopic level covering the full temperature range from room temperature to the upper application limit under monotonic as well as cyclic loading. Special emphasis is also put on the correct representation of the observed ratcheting behavior. The most prominent features of the model are the combined treatment of both, rate dependent, as well as rate independent inelasticity through a limit surface concept, the description of primary, secondary and tertiary creep behavior and the application of an appropriate backstress evolution equation. A fully implicit integration algorithm for the proposed model is developed and implemented in connection with uniaxial integration point drivers as well as three alternative Finite Element packages. The parameters of the proposed model can be identified based on a limited number of complex cyclic tests and monotonic creep tests. After reporting on the fitting results for such tests for the Nickel-base superalloy \{IN718\} the predictive capabilities of the proposed model are assessed for a number of isothermal and nonisothermal tests. Finally, the performance of the algorithmic implementation into the Finite Element packages is briefly addressed.},
  volume={27},
  year={2011},
  keywords={Numerical algorithms},
  pages={596 - 619},
}
