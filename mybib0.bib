@article{AbdelKarim2010711,
  doi={http://dx.doi.org/10.1016/j.ijplas.2009.10.002},
  author={Mohammad Abdel-Karim},
  title={An evaluation for several kinematic hardening rules on prediction of multiaxial stress-controlled ratchetting},
  url={http://www.sciencedirect.com/science/article/pii/S0749641909001260},
  journal={International Journal of Plasticity},
  issn={0749-6419},
  number={5},
  abstract={This study evaluates the performance of several non-linear kinematic hardening rules in predicting the various biaxial ratchetting experiments of stainless steel (SS) 304L under various stress-controlled histories performed by Hassan et al. (2008). The non-linear kinematic hardening rules proposed by Burlet and Cailletaud (1986), Ohno and Wang (1993, 1994), Abdel-Karim and Ohno (2000), Kang (2004), Chen and Jiao (2004), Chen et al. (2005) and the different rules of Abdel-Karim (2009) are examined and carefully scrutinized. The considered kinematic hardening rules range from the simple classical ones to more detailed rules, which incorporate additional terms and/or parameters to simulate different factors that affect ratchetting. It is shown that none of the examined kinematic hardening rules is general enough to simulate all of the ratchetting responses for the experiments under consideration.},
  volume={26},
  year={2010},
  keywords={Non-proportional biaxial loading},
  pages={711 - 730},
}
