@article{Kulawinski2015,
title = "Isothermal and thermo-mechanical fatigue behavior of the nickel base superalloy Waspaloy? under uniaxial and biaxial-planar loading ",
journal = "International Journal of Fatigue ",
volume = "81",
number = "",
pages = "21 - 36",
year = "2015",
note = "",
